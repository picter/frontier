<div id="winners" class="winner-section">
  <h1>{{title}}</h1>
  {{#each sections}}
    <div class="winner-item">
      <div class="caption">{{caption}}</div>
      <h2 class="title">{{title}}</h2>
      <div class="images">
        {{#each images}}
          <div class="winner-image">
            <img src={{image}} alt={{title}} />
            <div class="title">{{title}}</div>
          </div>
        {{/each}}
      </div>
    </div>
  {{/each}}
</div>
